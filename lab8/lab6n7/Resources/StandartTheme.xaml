<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes">
    
    <Style x:Key="HeaderStyle" TargetType="Grid">
        <Setter Property="Background" Value="#5AA9B0" />
    </Style>
    <Style x:Key="MenuStyle" TargetType="Grid">
        <Setter Property="Background" Value="#FF3E7585" />
    </Style>
    <Style x:Key="ButtonIconStyle" TargetType="materialDesign:PackIcon">
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style x:Key="ButtonIconPopUpStyle" TargetType="materialDesign:PopupBox">
        <Setter Property="Foreground" Value="White" />
    </Style>
</ResourceDictionary>